{
	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"declarationMap": true,
		"lib": ["dom", "es6"],
		"module": "commonjs",
		"moduleResolution": "node",
		"noEmitOnError": true,
		"noImplicitAny": true,
		"pretty": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"target": "es6"
	}
}
